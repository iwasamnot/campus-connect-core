    "@deepgram/sdk": "^4.11.3",
    "@google/generative-ai": "^0.24.1",
    "@pinecone-database/pinecone": "^6.1.3",
    "@react-spring/web": "^10.0.3",
    "@videosdk.live/react-sdk": "^0.6.7",
    "fabric": "^7.1.0",
    "firebase": "^12.7.0",
    "framer-motion": "^12.25.0",
    "gsap": "^3.14.2",
    "lucide-react": "^0.294.0",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",

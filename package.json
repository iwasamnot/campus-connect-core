{
  "name": "campusconnect",
  "private": true,
  "version": "8.3.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run fix:jsx",
    "fix:jsx": "node scripts/fix-jsx-extensions.js",
    "build:verify": "npm run build && npm run verify:build",
    "verify:build": "node scripts/verify-build.js",
    "preview": "vite preview",
    "predeploy": "npm run build:verify",
    "cap:sync": "npm run build && npx cap sync",
    "cap:ios": "npm run build && npx cap sync && npx cap open ios",
    "cap:android": "npm run build && npx cap sync && npx cap open android",
    "cap:copy": "npx cap copy",
    "cap:update": "npx cap update"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
